<instance format="XCSP3" type="CSP"> 
<variables>
<var id="MCS">0 1</var>
<var id="Memory">0 1</var>
<var id="Processor">0 1</var>
<var id="Cores">0..7</var>
<var id="Size">0..32</var>
</variables>
<constraints>
<intension id= "C2" >
	ge(MCS, Processor)
</intension>
<intension id= "C4" >
	ge(Processor, Memory)
</intension>
<intension id= "c6" >
	imp(Memory, gt(Size, 0))
</intension>
<intension id= "c7" >
	imp(Processor, gt(Cores, 0))
</intension>
</constraints>
</instance>
