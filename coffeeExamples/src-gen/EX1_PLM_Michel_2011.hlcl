model EX1_PLM_Michel_2011
variables:
boolean Document
boolean Type
boolean Binding
boolean Normal
boolean Booklet
boolean Top
boolean Left
boolean Right
constraints:
c0: Document = 1
c1: (Normal <=> (~Booklet AND Type)) AND (Booklet <=> (~Normal AND Type)) 
c2: (Top <=> (~Left AND~Right AND Binding)) AND (Left <=> (~Top AND~Right AND Binding)) AND (Right <=> (~Top AND~Left AND Binding)) 
c3:  ~ (Booklet AND Binding)
